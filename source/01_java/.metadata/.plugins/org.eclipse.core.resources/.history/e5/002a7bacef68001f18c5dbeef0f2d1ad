package com.lec.ex1_car;

public class Carmain {
	public static void main(String[] args) {
		Car myPorsche = new Car(); // new 이용하여 메모리 안착, ex. color = 0 
		myPorsche.color = "보라";
		myPorsche.cc = 2400;
		System.out.println(myPorsche.color + "색차량 배기량은 " +
				           myPorsche.cc + ", 속도는 " + myPorsche.speed);
		myPorsche.drive();
		myPorsche.park();
		myPorsche.race();
		System.out.println(myPorsche.color + "색차량 배기량은 " +
		           myPorsche.cc + ", 속도는 " + myPorsche.speed);	
		
		Car youPorsche = new Car();
		
	} // main
} // class
