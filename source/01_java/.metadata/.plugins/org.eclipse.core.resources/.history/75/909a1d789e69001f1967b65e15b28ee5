package com.lec.ex5_person;
// 데이터: name, age, gender, birth(String → 13장 수업후에는 Date형)
// 생성자 : new PersonInfo("홍길동", 22, '남', "2001-12-01") 
public class PersonInfo {
	// 데이터(속성)
	private String name;
	private int    age;
	private char   gender;
	private String birth;
	
	// 생성자 함수
	Public PersonInfo( ) { }
	Public PersonInfo( ) {
		
	}
}
