package com.lec.ex0_janmuri;

class PersonInfo{
	private String name; 
	private String tel; // 데이터
	public PersonInfo(String name, String tel) {
		this.name = name;
		this.tel = tel;
	} // 생성자 함수
	public String info() {
		return name + ":" + tel;
	} // method
}
