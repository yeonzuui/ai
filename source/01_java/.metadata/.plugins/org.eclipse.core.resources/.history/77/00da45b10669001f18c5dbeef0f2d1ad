package com.lec.method;
// 수학적 method만 모아놓은 class => Arithmetic
public class ArPractice {
	// method 1) abs(절댓값; absolute value)
	public static int abs(int value) {
		int result = (value >= 0) ? value : -value;
		return result;
	}
	
	// method 2) sum / overloading(과적)
