package com.lec.quiz;

// 배열 안의 값의 합계, 평균, 최댓값, 최솟값 출력
public class Quiz2 {
	public static void main(String[] args) {
		int[] arr = { 10, 20, 30, 40, 50, 12, 13 };
		int sum = 0;
		for (int i = 0; i < arr.length; i++) {
			sum += arr[i];
			System.out.println("sum = " + sum);
		}
	}// main
}// class
