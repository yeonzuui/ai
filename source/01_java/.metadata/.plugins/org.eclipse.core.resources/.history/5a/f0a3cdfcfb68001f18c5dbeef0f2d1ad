package com.lec.ex2_rect;
// class : 속성(인스턴스변수), 생성자함수(속성데이터초기화), method, setter&getter
// Rect r1 = new Rect(); r1.setWidth(2); r1.setHeight(4);
// Rect r1 = new Rect(2, 4)
// Rect r1 = new Rect(3);
// 생성자함수도 overloading 가능(매개변수 X, 매개변수 1개, 매개변수 2개,....)
public class Rect {
	private int width;
	private int height;
	// 생성자 함수가 없을 경우, jVM이 디폴트 생성자 자동 생성
	public Rect( ) {} // 디폴트 생성자
	public Rect(int side) {
		width  = side;
		height = side;
	}
	public Rect(int w, int h) {
		this.width  = width;
		this.height = height;
	}
}
