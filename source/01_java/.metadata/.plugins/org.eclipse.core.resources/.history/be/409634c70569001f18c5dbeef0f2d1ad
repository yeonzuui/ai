package com.lec.ex;

import com.lec.method.Arithmetic;

public class Ex03Practice {
	public static void main(String[] args) {
		Arithmetic ar = new Arithmetic(); 
		// ar이라는 객체에 Arithmetic class폴더에 있는 Arithmetic 생성자함수 들고와, 초기화 작업
		int tot = ar.sum(100); // tot(정수변수)에 1~100까지 누적합 할당
		System.out.println("-9의 절댓값: " + Arithmetic.abs(-9));
		System.out.println("합은 " + tot);
		System.out.println(ar.evenOdd(tot));
	}
}
